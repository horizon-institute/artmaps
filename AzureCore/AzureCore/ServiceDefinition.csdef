<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="AzureCore" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-05.1.7">
  <WebRole name="UI" vmsize="Medium">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="http" endpointName="http" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="http" protocol="http" port="80" />
    </Endpoints>
    <Imports>
      <Import moduleName="Diagnostics" />
      <Import moduleName="Caching" />
    </Imports>
    <ConfigurationSettings>
      <Setting name="ArtMaps.DevFabric.Tracing.Path" />
      <Setting name="ArtMaps.Diagnostics.BufferQuota" />
      <Setting name="ArtMaps.Diagnostics.LogLevel" />
      <Setting name="ArtMaps.Diagnostics.TransferPeriod" />
      <Setting name="ArtMaps.Security.KeySize" />
      <Setting name="ArtMaps.SqlServer.ConnectionString" />
      <Setting name="ArtMaps.Storage.ConnectionString" />
      <Setting name="ArtMaps.Storage.Containers" />
      <Setting name="ArtMaps.Storage.Queues" />
      <Setting name="ArtMaps.Storage.Tables" />
    </ConfigurationSettings>
    <Certificates>
    </Certificates>
    <LocalResources>
      <LocalStorage name="Microsoft.WindowsAzure.Plugins.Caching.FileStore" sizeInMB="1000" cleanOnRoleRecycle="false" />
      <LocalStorage name="DiagnosticStore" cleanOnRoleRecycle="false" sizeInMB="8192" />
    </LocalResources>
  </WebRole>
</ServiceDefinition>